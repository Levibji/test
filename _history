e.createSprite(x, y)\n}\n\n// Schlange bewegen\nfunction bewegeSchlange() {\n    if (gameOver) {\n        return\n    }\n\n    let kopf = snake[0]\n    let neueX = kopf.getX()\n    let neueY = kopf.getY()\n\n    // Neue Position der Schlange basierend auf der Richtung\n    if (richtung == 0) {\n        neueY -= 1  // nach oben\n    } else if (richtung == 1) {\n        neueX += 1  // nach rechts\n    } else if (richtung == 2) {\n        neueY += 1  // nach unten\n    } else if (richtung == 3) {\n        neueX -= 1  // nach links\n    }\n\n    // Prüfen, ob die Schlange den Rand berührt oder sich selbst\n    if (neueX < 0 || neueX > 4 || neueY < 0 || neueY > 4 || schlangenKollision(neueX, neueY)) {\n        gameOver = true\n        game.showString(\"Game Over\")\n        return\n    }\n\n    // Neue Position für den Kopf der Schlange\n    let neuesKopf = game.createSprite(neueX, neueY)\n    snake.unshift(neuesKopf)  // Kopf an den Anfang der Liste hinzufügen\n\n    // Wenn die Schlange das Essen frisst\n    if (neueX == essen.getX() && neueY == essen.getY()) {\n        essen.delete()\n        spawnEssen()  // Neues Essen spawnen\n    } else {\n        // Wenn die Schlange nicht wächst, das letzte Segment entfernen\n        let schwanz = snake.pop()\n        schwanz.delete()\n    }\n}\n\n// Prüfen, ob die Schlange sich selbst berührt\nfunction schlangenKollision(x: number, y: number): boolean {\n    for (let i = 1; i < snake.length; i++) {\n        if (snake[i].getX() == x && snake[i].getY() == y) {\n            return true\n        }\n    }\n    return false\n}\n\n// Steuerung der Schlange\ninput.onButtonPressed(Button.A, function () {\n    if (richtung != 1) {\n        richtung = 3  // links\n    }\n})\ninput.onButtonPressed(Button.B, function () {\n    if (richtung != 3) {\n        richtung = 1  // rechts\n    }\n})\ninput.onLogoEvent(TouchButtonEvent.Pressed, function () {\n    if (richtung != 2) {\n        richtung = 0  // oben\n    }\n})\ninput.onButtonPressed(Button.AB, function () {\n    if (richtung != 0) {\n        richtung = 2  // unten\n    }\n})\n\n// Spiel starten\nsnakeInitialisieren()\nbasic.forever(function () {\n    if (!gameOver) {\n        bewegeSchlange()\n        basic.pause(500)  // Geschwindigkeit der Schlange\n    }\n})\n"],[0,"\n"]],"start1":0,"start2":0,"length1":2736,"length2":1}]},{"type":"edited","filename":"pxt.json","patch":[{"diffs":[[0,"itor\": \""],[-1,"t"],[1,"block"],[0,"sprj\"\n}\n"]],"start1":213,"start2":213,"length1":17,"length2":21}]}]},{"timestamp":1751531790192,"editorVersion":"7.0.3","changes":[{"type":"edited","filename":"main.blocks","patch":[{"diffs":[[0,"id=\""],[-1,"wBCPIlyIeYQUPsi77E,-\">snake</variable><variable id=\"mY.=vNBd]%^OVP%wyFVT\">gameOver</variable><variable id=\"%km2/Gnb-X{driQD_cbX\">richtung</variable><variable id=\"5Pg2%+:SHH6R.MiY3P|l\">x</variable><variable id=\",Kt4OWzOBk0yyZUTCbzs\">y</variable><variable id=\"wza!gxDn_L?-~vTdSSie\">essen</variable><variable id=\"4WK^uh|M6fv~rg}H-n=N\">kopf</variable><variable id=\"feXNg/}`:UBamY|0$w.o\">neueX</variable><variable id=\"-o{LoVQXv`heV)[i0_~5\">neueY</variable><variable id=\"rQJx)}oQI.Bt.ut`O9=N\">neuesKopf</variable><variable id=\"Kuyxbux@Hy]!`;|ufqXi\">schwanz</variable></variables><comment x=\"20\" y=\"3258\" w=\"420\" h=\"120\">\n0 = oben, 1 = rechts, 2 = unten, 3 = links\n</comment><block type=\"pxt-on-start\" x=\"-132\" y=\"-205\"><statement name=\"HANDLER\"><block type=\"typescript_statement\"><mutation xmlns=\"http://www.w3.org/1999/xhtml\" line0=\"let snake: game.LedSprite[] = []\" numlines=\"1\" declaredvars=\"snake\"></mutation><next><block type=\"function_call\"><mutation name=\"snakeInitialisieren\" functionid=\"-dM=R)]39Gw%L`.)*+Uo\"></mutation><comment pinned=\"false\" h=\"80\" w=\"160\">Spiel starten</comment></block></next></block></statement></block><block type=\"device_button_event\" x=\"311\" y=\"-216\"><field name=\"NAME\">Button.A</field><comment pinned=\"false\" h=\"80\" w=\"160\">Steuerung de